  object IBQuery1: TIBQuery
    Database = DataGeneral.IBDatabase1
    Transaction = IBTransaction2
    SQL.Strings = (
      'SELECT '
      '  "fun$tipocapacitacion"."descripcion",'
      '  "fun$capacitacion"."descripcion",'
      '  "fun$capacitacion"."fecha",'
      '  "fun$capacitacion"."horario",'
      '  "fun$capacitacion"."horas",'
      '  "fun$capacitacion"."esjuvenil",'
      '  "fun$capacitacion"."lugar",'
      '  COUNT("fun$capacitacionper"."id_persona") AS CUPO'
      'FROM'
      '  "fun$capacitacion"'
      
        '  INNER JOIN "fun$tipocapacitacion" ON ("fun$capacitacion"."id_t' +
        'ipocapacitacion" = "fun$tipocapacitacion"."id_tipo")'
      
        '  INNER JOIN "fun$capacitacionper" ON ("fun$capacitacion"."id_ca' +
        'pacitacion" = "fun$capacitacionper"."id_capacitacion")'
      'WHERE'
      '  ("fun$capacitacion"."fecha" BETWEEN :FECHA1 AND :FECHA2)'
      'GROUP BY'
      '  "fun$tipocapacitacion"."descripcion",'
      '  "fun$capacitacion"."descripcion",'
      '  "fun$capacitacion"."fecha",'
      '  "fun$capacitacion"."horario",'
      '  "fun$capacitacion"."horas",'
      '  "fun$capacitacion"."esjuvenil",'
      '  "fun$capacitacion"."lugar"')
