  object Ibbuscapol: TIBQuery
    Database = DataGeneral.IBDatabase1
    Transaction = DataGeneral.IBTransaction1
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'SELECT DISTINCT '
      '  "act$activo"."cod_activo",'
      '  "act$activo"."descripcion" AS "descripcion1",'
      '  "act$poliza"."fecha_inicio",'
      '  "act$poliza"."fecha_vencimiento",'
      '  "act$poliza"."valor_asegurado",'
      '  "act$tipo_poliza"."cod_tipopoliza" AS "tipo"'
      'FROM'
      '  "act$activo",'
      '  "act$poliza",'
      '  "act$tipo_poliza",'
      '  "inv$dependencia",'
      '  "Inv$Agencia",'
      '  "act$traslado"'
      'WHERE'
      '  ("act$activo"."cod_activo" = "act$poliza"."cod_activo") AND '
      
        '  ("act$tipo_poliza"."cod_tipopoliza" = "act$poliza"."tipo_poliz' +
        'a") AND '
      '  ("act$poliza"."vencido" = 0) AND '
      '  ("act$traslado"."lugar" = '#39'A'#39') AND '
      '  ("act$activo"."estado" = '#39'A'#39') AND '
      '  ("act$activo"."placa" = :placa)')
    Left = 304
    Top = 128
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'placa'
        ParamType = ptUnknown
      end>
  end
  object Ibcancela: TIBQuery
    Database = DataGeneral.IBDatabase1
    Transaction = DataGeneral.IBTransaction1
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'update "act$poliza" set  "act$poliza"."vencido" = 1'
      'where "act$poliza"."cod_activo"=:"cod_activo"'
      'and "act$poliza"."tipo_poliza"=:"cod_poliza"')
    Left = 304
    Top = 80
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_activo'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'cod_poliza'
        ParamType = ptUnknown
      end>
  end
